<div class="row">
  <div class="col-lg-12">
  <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h3>ติดตามเอกสาร</h3>
      </div>
      <div class="ibox-content">
        <div class="row">
          <div class="col-lg-12">
            <div class="tabs-container">
              <ul class="nav nav-tabs">
                <li class="active">
                  <a data-toggle="tab" href="#tab-1" aria-expanded="false">รับเข้าระบบแล้ว</a>
                </li>
                <li>
                  <a data-toggle="tab" href="#tab-2" aria-expanded="false">ส่งมอบแผนกที่เกี่ยวข้องแล้ว</a>
                </li>
                <li>
                  <a data-toggle="tab" href="#tab-3" aria-expanded="false">ดำเนินการเสร็จสิ้นแล้ว</a>
                </li>
              </ul>
              <div class="tab-content">
                <div id="tab-1" class="tab-pane active">
                  <div class="panel-body">
                    <div class="ibox folat-e-margins">
                      <div class="ibox-title">
                        <h3>เอกสารที่รับเข้าแล้ว</h3>
                      </div>
                      <div class="ibox-content">
                        <div class="table-responsive">
                          <div id="DataTables_Table_0_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                            <div id="DataTables_Table_0_filter" class="dataTables_filter">
                              <!-- <label>
                                "ค้นหา:"
                                <input type="search" class="form-control input-sm" aria-controls="DataTables_Table_0">
                              </label> -->
                            </div>
                            <table class="table table-striped table-bordered table-hover dataTables-example dataTable" id="DataTables_Table_0"
                            aria-describedby="DataTables_Table_0_info" role="grid">
                            <thead>
                              <tr>
                                <th>หัวข้อ</th>
                                <th>ประเภทของเรื่อง</th>
                                <th>ประจำแผนก</th>
                                <th>สถานะ</th>
                                <th>เมื่อวันที่</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% @received_document.each do |n| %>
                              <tr>
                                <td><%= n.title %></td>
                                <td>
                                  <% n.type.each do |type| %>
                                    <p><%= type.name %></p>
                                  <% end %>
                                </td>
                                <td></td>
                                <td><%= n.status_check %></td>
                                <td>
                                  <!--  -->
                                </td>
                              </tr>
                              <% end %>
                            </tbody>
                            </table>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div id="tab-2" class="tab-pane">
                  <div class="panel-body">
                    <div class="ibox folat-e-margins">
                      <div class="ibox-title">
                        <h3>ส่งมอบแผนกที่เกี่ยวข้องแล้ว</h3>
                      </div>
                      <div class="ibox-content">
                        <div class="table-responsive">
                          <div id="DataTables_Table_0_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                            <div id="DataTables_Table_0_filter" class="dataTables_filter">
                              <!-- <label>
                                "ค้นหา:"
                                <input type="search" class="form-control input-sm" aria-controls="DataTables_Table_0">
                              </label> -->
                            </div>
                            <table class="table table-striped table-bordered table-hover dataTables-example dataTable" id="DataTables_Table_0"
                            aria-describedby="DataTables_Table_0_info" role="grid">
                            <thead>
                              <tr>
                                <th>หัวข้อ</th>
                                <th>ประเภทของเรื่อง</th>
                                <th>ประจำแผนก</th>
                                <th>สถานะ</th>
                                <th>เมื่อวันที่</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% @send_department.each do |n| %>
                              <tr>
                                <td><%= n.title %></td>
                                <td>
                                  <% n.type.each do |type| %>
                                    <p><%= type.name %></p>
                                  <% end %>
                                </td>
                                <td>
                                  <% n.department.each do |n| %>
                                    <p><%= n.name %></p>
                                  <% end %>
                                </td>
                                <td><%= n.status_check %></td>
                                <td>
                                  <!--  -->
                                </td>
                              </tr>
                              <% end %>
                            </tbody>
                            </table>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div id="tab-3" class="tab-pane">
                  <div class="panel-body">
                    <div class="ibox folat-e-margins">
                      <div class="ibox-title">
                        <h3>ดำเนินการเสร็จสิ้นแล้ว</h3>
                      </div>
                      <div class="ibox-content">
                        <div class="table-responsive">
                          <div id="DataTables_Table_0_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                            <div id="DataTables_Table_0_filter" class="dataTables_filter">
                              <!-- <label>
                                "ค้นหา:"
                                <input type="search" class="form-control input-sm" aria-controls="DataTables_Table_0">
                              </label> -->
                            </div>
                            <table class="table table-striped table-bordered table-hover dataTables-example dataTable" id="DataTables_Table_0"
                            aria-describedby="DataTables_Table_0_info" role="grid">
                            <thead>
                              <tr>
                                <th>หัวข้อ</th>
                                <th>ประเภทของเรื่อง</th>
                                <th>ประจำแผนก</th>
                                <th>สถานะ</th>
                                <th>เมื่อวันที่</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% @complete_document.each do |n| %>
                              <tr>
                                <td><%= n.title %></td>
                                <td>
                                  <% n.type.each do |type| %>
                                    <p><%= type.name %></p>
                                  <% end %>
                                </td>
                                <td><%= n.status_check %></td>
                                <td>
                                  <!--  -->
                                </td>
                              </tr>
                              <% end %>
                            </tbody>
                            </table>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
  </div>
  </div>
</div>
